{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        },
        {
            "src": "/api/v1/:cetegory/(.*)",
            "dest": "/routes/api/v1/category.route.js",
            "methods": ["GET"]
        
        }
    ]
}
