{
    "version": 2,
    "builds": [
        {
            "src": "./app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/v1/category",
            "dest": "/routes/api/v1/category.route.js"
        },
        {
            "src": "/(.*)",
            "dest": "/app.js"
        }
    ]
}
