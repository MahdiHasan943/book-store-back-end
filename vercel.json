{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
          },
        {
            "src": "/api/v1/category/.*",
            "dest": "./index.js"
        },
        {
            "src": "/api/v1/store/.*",
            "dest": "./index.js"
        },
        {
            "src": "/api/v1/brand/.*",
            "dest": "./index.js"
        },
        {
            "src": "/api/v1/stock/.*",
            "dest": "./index.js"
        },
        {
            "src": "/api/v1/products/.*",
            "dest": "./index.js"
        },
        {
            "src": "/.*",
            "dest": "./index.js"
        }
    ]
}
