{
    "version": 2,
    "builds": [
        {
            "src": "./app.js",
            "use": "@vercel/node"
        }
    ],
,
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        },
        {
            "src": "/routes/api/v1/category.route/(.*)",
            "dest": "/api/v1/category "

        }
    ]
}
