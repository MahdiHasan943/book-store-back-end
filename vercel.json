{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/v1/category",
            "dest": "/index.js" 
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}