{
    "version": 2,
    "builds": [
        {
            "src": "./app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "handle": "filesystem"
          },
          {
            "src": "/api/v1/:cetegory/(.*)",
            "dest": "/routes/v1/:cetegory/$1"
          }
      
    ]
}
